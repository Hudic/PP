<mapper namespace="com.wms.test.mapper.BookMapper">
  <resultMap id="baseResult" type="com.wms.test.domain.Book">
    <id column="id" property="id" jdbcType="BIGINT" />
    <result column="name" property="name" jdbcType="VARCHAR" />
    <result column="number" property="number" jdbcType="VARCHAR" />
    <result column="price" property="price" jdbcType="VARCHAR" />
  </resultMap>

  <insert id="insert">
    insert  into book(id,name,number,price) values(#{id},#{name},#{number},#(price));
  </insert>

  <select id="selectAll" resultMap="baseResult">
    select id,name,number,price from book
  </select>

</mapper>